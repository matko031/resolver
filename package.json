{
   "name": "resolver",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "generate-types": "openapi-typescript specs/spec.yml --output src/types/api.ts",
      "dev": "nodemon src/server.ts",
      "build": "tsc && tsc-alias && cpy specs/* ./dist",
      "start": "ts-node -r tsconfig-paths/register src/server",
      "serve": "node -r tsconfig-paths/register dist/src/server.js",
      "openapi:build": "redocly build-docs ./specs/spec.yml -o ./specs/public/index.html",
      "test": "docker exec -t resolver-api npx jest",
      "prettier:format": "docker exec -t resolver-api npx prettier --write \"src/**/*.{ts,tsx,js,md,mdx,css,yaml}\"",
      "prettier:check": "docker exec -t resolver-api npx prettier --check \"src/**/*.{ts,tsx,js,md,mdx,css,yaml}\""
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "dependencies": {
      "@redocly/cli": "^1.5.0",
      "@types/lodash": "^4.14.198",
      "@types/morgan": "^1.9.5",
      "@types/node": "^20.10.6",
      "@types/ua-parser-js": "^0.7.37",
      "axios": "^1.6.2",
      "cpy": "^10.1.0",
      "cpy-cli": "^5.0.0",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "express-openapi-validator": "^5.0.6",
      "lodash": "^4.17.21",
      "mariadb": "^3.2.2",
      "morgan": "^1.10.0",
      "mysql2": "^3.6.1",
      "redoc": "^2.1.3",
      "sequelize": "^6.33.0",
      "tsc-alias": "^1.8.7",
      "tsconfig-paths": "^4.2.0",
      "ua-parser": "^0.3.5",
      "ua-parser-js": "^1.0.36",
      "winston": "^3.10.0",
      "winston-daily-rotate-file": "^4.7.1",
      "winston-transport": "^4.5.0"
   },
   "devDependencies": {
      "@types/express": "^4.17.21",
      "@types/jest": "^29.5.11",
      "@types/supertest": "^6.0.0",
      "jest": "^29.7.0",
      "nodemon": "^3.0.2",
      "openapi-typescript": "^6.5.5",
      "prettier": "^3.1.1",
      "supertest": "^6.3.3",
      "ts-jest": "^29.1.1",
      "ts-node": "^10.9.2",
      "typescript": "^5.3.3"
   }
}
